swapsize=$(dialog --no-cancel --backtitle "$titulo" --title "-| Cantidad de Home y Swap |-" \
--form "\nIngresa la cantidad de Home y Swap\n\nGigabytes (G) - Megabytes (M) - Kibibyte (K)\n\n> No deje espacios al inicio\n> No olvidar su etiqueta al final del número\n\
> [Tabulador] o [FLECHA ARRIBA] / [FLECHA ABAJO]\n\nGigabytes disponibles ${sizedisk}\n " 0 0 0 \
" Tamaño en GB para HOME:" 1 1 "25G" 1 26 25 20 2>&1 > /dev/tty \
"Tamaño en GB para SWAP: " 3 2 "2G" 3 26 25 20 3>&1 > /dev/tty)

clear
echo $swapsize