localeskeymapx="$(cat lib/list_X11keymap | awk '$localeskeymapx=$localeskeymapx" x11-keymap"')" && localeskeymapx=$(echo "$localeskeymapx") 
        keyboardx=$(dialog --backtitle "${title}" --clear --no-cancel --title "${title_langXorg}" --menu " \n " 15 50 15   ${localeskeymapx} 2>&1 > /dev/tty) 
        